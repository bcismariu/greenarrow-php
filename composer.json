{
    "name": "bcismariu/greenarrow-php",
    "description": "Simple PHP wrapper for GreenArrow's sending email API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "greenarrow"
    ],
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": ">=0.9.4"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Bcismariu\\GreenArrow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bcismariu\\GreenArrow\\Tests\\": "tests/"
        }
    }
}
